{
  "name": "prisma-generator-express",
  "description": "Prisma generator of Express CRUD API",
  "version": "1.16.6",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": "dist/bin.js",
  "engines": {
    "node": ">=20.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "yarn build",
    "test": "jest",
    "coverage": "jest --coverage",
    "prepublishOnly": "node copy.js "
  },
  "dependencies": {
    "@prisma/client": "6.17.1",
    "@prisma/generator-helper": "6.17.1",
    "@prisma/sdk": "4.0.0",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "prettier": "3.6.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@prisma/internals": "^6.17.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/express": "^5.0.3",
    "@types/jest": "30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "24.7.2",
    "@types/prettier": "3.0.0",
    "jest": "30.2.0",
    "prisma": "6.17.1",
    "semantic-release": "^24.2.9",
    "ts-jest": "29.4.5",
    "typescript": "5.9.3"
  },
  "homepage": "https://github.com/multipliedtwice/prisma-generator-express/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/multipliedtwice/prisma-generator-express.git"
  },
  "author": "multipliedtwice",
  "keywords": [
    "prisma",
    "express",
    "crud",
    "generator"
  ],
  "files": [
    "dist/**/*",
    "src/**/*",
    "../../README.md",
    "../../LICENSE"
  ],
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
